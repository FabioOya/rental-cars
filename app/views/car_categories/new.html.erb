<%= form_for @car_category do |c| %>
  <%= c.label :name, 'Nome' %>
  <%= c.text_field :name %>
  <%= c.label :daily_rate, 'Taxa Diaria' %>
  <%= c.text_field :daily_rate %>
  <%= c.label :car_insurance, 'Seguro' %>
  <%= c.text_field :car_insurance %>
  <%= c.label :third_party_insurance, 'Seguro de terceiros' %>
  <%= c.text_field :third_party_insurance %>
  <%= c.submit 'Enviar' %>
<% end %>
